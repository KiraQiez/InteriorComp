<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>CheckLogin</servlet-name>
        <servlet-class>com.interior.controller.CheckLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateRoom</servlet-name>
        <servlet-class>com.interior.controller.CreateRoom</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.interior.controller.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>insertBooking</servlet-name>
        <servlet-class>com.interior.controller.insertBooking</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>staffUpdateReport</servlet-name>
        <servlet-class>com.interior.controller.staffUpdateReport</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>studentReport</servlet-name>
        <servlet-class>com.interior.controller.studentReport</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>studentRoomDetails</servlet-name>
        <servlet-class>com.interior.controller.studentRoomDetails</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckLogin</servlet-name>
        <url-pattern>/CheckLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateRoom</servlet-name>
        <url-pattern>/CreateRoom</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>insertBooking</servlet-name>
        <url-pattern>/insertBooking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>staffUpdateReport</servlet-name>
        <url-pattern>/staffUpdateReport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>studentReport</servlet-name>
        <url-pattern>/studentReport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>studentRoomDetails</servlet-name>
        <url-pattern>/studentRoomDetails</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>roomS.jsp</welcome-file>
    </welcome-file-list>
</web-app>
